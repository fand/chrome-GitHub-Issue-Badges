{
  "name": "Embed GitHub Issue Badges",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "",
  "permissions": [
    "https://github.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [ "" ],
      "js": [ "js/inject.js" ]
    }
  ]
}
